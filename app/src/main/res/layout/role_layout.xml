                android:layout_weight="1"
                app:srcCompat="@drawable/ic_sync_black_24dp" />

            <ImageView
                android:id="@+id/hwevo1"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_weight="1"
                android:visibility="invisible"
                app:srcCompat="@drawable/ic_sync_black_24dp" />

            <ImageView
                android:id="@+id/hwevo2"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_weight="1"
                android:visibility="invisible"
