<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/lightgraycgg">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/statistics"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:fontFamily="sans-serif-condensed"
            android:text="Statistics"
            android:textAlignment="center"
            android:textColor="@android:color/background_light"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/winRate"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@color/graycgg"
                android:text="Win Rate"
                android:textAlignment="textStart"
                android:textColor="@android:color/background_light"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/winRateNumber"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@color/graycgg"
                android:text="TextView"
                android:textColor="@android:color/background_light"
                android:textSize="18sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/playRate"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@color/customgray"
                android:text="Play Rate"
                android:textAlignment="textStart"
                android:textColor="@android:color/background_light"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/playRateNumber"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@color/customgray"
                android:text="TextView"
                android:textColor="@android:color/background_light"
                android:textSize="18sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/banRate"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@color/graycgg"
                android:text="Ban Rate"
                android:textAlignment="textStart"
                android:textColor="@android:color/background_light"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/banRateNumber"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@color/graycgg"
                android:text="TextView"
                android:textColor="@android:color/background_light"
                android:textSize="18sp" />
        </LinearLayout>

        <TextView
            android:id="@+id/itembuilds"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginTop="8dp"
            android:fontFamily="sans-serif-condensed"
            android:text="Item Builds"
            android:textAlignment="center"
            android:textColor="@android:color/background_light"
            android:textSize="24sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="442dp" />

        <TextView
            android:id="@+id/freqcomp"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:background="@color/graycgg"
            android:text="Frequently Completed"
            android:textAlignment="center"
            android:textColor="@android:color/background_light"
            android:textSize="18sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/fcitem1"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_sync_black_24dp" />

            <ImageView
                android:id="@+id/fcitem2"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_sync_black_24dp" />

            <ImageView
                android:id="@+id/fcitem3"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_sync_black_24dp" />

            <ImageView
                android:id="@+id/fcitem4"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_sync_black_24dp" />

            <ImageView
                android:id="@+id/fcitem5"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_sync_black_24dp" />

            <ImageView
                android:id="@+id/fcitem6"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_sync_black_24dp" />
        </LinearLayout>

        <TextView
            android:id="@+id/hw"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:background="@color/graycgg"
            android:text="Highest Winrate"
            android:textAlignment="center"
            android:textColor="@android:color/background_light"
            android:textSize="18sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/hwitem1"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_sync_black_24dp" />

            <ImageView
                android:id="@+id/hwitem2"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_sync_black_24dp" />

            <ImageView
                android:id="@+id/hwitem3"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_sync_black_24dp" />

            <ImageView
                android:id="@+id/hwitem4"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_sync_black_24dp" />

            <ImageView
                android:id="@+id/hwitem5"
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_sync_black_24dp" />

            <ImageView
                android:id="@+id/hwitem6"
                android:layout_width="65dp"
