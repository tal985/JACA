                android:orientation="vertical">

                <ImageView
                    android:id="@+id/fsrune1"
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    app:srcCompat="@drawable/ic_sync_black_24dp" />

                <ImageView
                    android:id="@+id/fsrune2"
                    android:layout_width="65dp"
